{
	"name": "@graphext/cuery",
	"version": "0.5.0",
	"license": "MIT",
	"exports": {
		".": "./mod.ts"
	},
	"imports": {
		"@openai/openai": "npm:openai@^6.9.1",
		"@google/genai": "npm:@google/genai@^1.37.0",
		"@zod/zod": "jsr:@zod/zod@^4.3",
		"tldts": "npm:tldts@^6.1.71",
		"cheerio": "npm:cheerio@^1.1.2",
		"google-auth-library": "npm:google-auth-library@^10.4.0",
		"@std/path": "jsr:@std/path@^1",
		"@std/assert": "jsr:@std/assert@^1",
		"@std/dotenv": "jsr:@std/dotenv@^0.225"
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"singleQuote": true,
		"proseWrap": "preserve"
	},
	"lint": {
		"rules": {
			"exclude": [
				"no-slow-types"
			]
		}
	},
	"tasks": {
		"test": "deno test --allow-env --allow-net --env-file",
		"check": "deno check ./mod.ts",
		"run": "deno run --allow-env --allow-net --env-file --allow-read --allow-write",
		"publish": "deno publish --allow-slow-types",
		"build:npm": "deno run -A scripts/build_npm.ts",
		"publish:npm": "deno task build:npm && cd npm && npm publish --access public --registry https://registry.npmjs.org/",
		"update-models": "deno run --allow-net --allow-write scripts/update-models.ts"
	}
}